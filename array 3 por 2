#include <stdio.h>
#include <string.h>
int main()
{
    int x, y, num[3][2],z;
    for(x=0;x<3;x++){
        for(y=0;y<2;y++){
            printf("Digite um numero:");
            scanf("%d",&num[x][y]);
        }
    }
    for(x=0;x<3;x++){
        printf("\n");
        for(y=0;y<2;y++){
            
            printf("%d ",num[x][y]);  
            
            
        }
    }

    return 0;
}


